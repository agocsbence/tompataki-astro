{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/search/?$",
			"dest": "_render"
		}
	],
	"images": {
		"sizes": [
			640,
			750,
			828,
			1080,
			1200,
			1920,
			2048,
			3840
		],
		"domains": [
			"cms.tompataki.com"
		],
		"remotePatterns": [
			{
				"protocol": "https",
				"hostname": "cms.tompataki.com",
				"pathname": "/**"
			},
			{
				"protocol": "https",
				"hostname": "tompataki.com",
				"pathname": "/**"
			},
			{
				"protocol": "https",
				"hostname": "i.vimeocdn.com",
				"pathname": "/**"
			}
		]
	}
}